<block>
  <name>QT GUI Range Velocity Diagram</name>
  <key>radar_qtgui_rv_diagram</key>
  <category>RADAR</category>
  <import>import radar</import>
  <make>radar.qtgui_rv_diagram($interval, $axis_range, $axis_velocity)</make>
  <param>
    <name>Update interval</name>
    <key>interval</key>
    <type>int</type>
  </param>
  <param>
    <name>Axis Range</name>
    <key>axis_range</key>
    <type>real_vector</type>
  </param>
  <param>
    <name>Axis Velocity</name>
    <key>axis_velocity</key>
    <type>real_vector</type>
  </param>
  <sink>
    <name>Msg in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
</block>
