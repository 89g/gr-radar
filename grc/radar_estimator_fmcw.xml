<block>
  <name>Estimator FMCW</name>
  <key>radar_estimator_fmcw</key>
  <category>RADAR</category>
  <import>import radar</import>
  <make>radar.estimator_fmcw($samp_rate, $center_freq, $sweep_freq, $samp_up, $samp_down)</make>
  <param>
    <name>Sample rate</name>
    <key>samp_rate</key>
    <type>int</type>
  </param>
  <param>
    <name>Center frequency</name>
    <key>center_freq</key>
    <type>float</type>
  </param>
  <param>
    <name>Sweep frequency</name>
    <key>sweep_freq</key>
    <type>float</type>
  </param>
  <param>
    <name>Samples up-chirp</name>
    <key>samp_up</key>
    <type>int</type>
  </param>
  <param>
    <name>Samples down-chirp</name>
    <key>samp_down</key>
    <type>int</type>
  </param>
  <sink>
    <name>Msg in CW</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <sink>
    <name>Msg in UP</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <sink>
    <name>Msg in DOWN</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>Msg out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
